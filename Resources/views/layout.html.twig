	<script src="{{ asset('bundles/zzpyro/js/lib/jquery_regex_selector.js') }}"></script>
{% endblock %}

{% block javascripts %}
	{{ parent() }}
	<script src="{{ asset('bundles/zzpyro/js/form/collections.js') }}"></script>
	<script src="{{ asset('bundles/zzpyro/js/form/select_or_text.js') }}"></script>
	<script src="{{ asset('bundles/zzpyro/js/form/transform.js') }}"></script>
	<script src="{{ asset('bundles/zzpyro/js/form/elements.js') }}"></script>
{% endblock %}
