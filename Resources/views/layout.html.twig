{% extends 'ZzCoreBundle::base.html.twig' %}

{% trans_default_domain 'ZzPyroBundle' %}

{% block title %}{{ 'title' |trans }} - {{ parent() }}{% endblock %}

{% block css_lib %}
	{{ parent() }}
	{# {% stylesheets 'bundles/zzpyro/css/bootstrap/dist/css/bootstrap.css' filter='cssrewrite' %}
		<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %} #}
</head>
{% endblock %}

{% block js_lib %}
	{{ parent() }}
	<script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
